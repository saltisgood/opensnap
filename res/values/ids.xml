<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- A set of ids to be used for embedding information in views -->

	<!-- The position of the snap in its respective snap list -->
	<item
	    type="id"
	    name="snap_item_position" />
    <!-- The key used to store a reference to a snap object inside the list or whatever -->
    <item
        type="id"
        name="snap_item" />
    <!-- Key used for whether an imageview is an animation -->
    <item
        type="id"
        name="img_anim" />
    <!-- Key used for storing the resource id of an imageview -->
    <item
        type="id"
        name="img_res_id" />
</resources>
